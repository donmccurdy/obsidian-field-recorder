{
	"name": "obsidian-field-recorder",
	"version": "1.0.0",
	"description": "Field audio recorder with extended audio quality settings and waveform visualization.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint . && biome check",
		"format": "biome check --write"
	},
	"keywords": [],
	"license": "UNLICENSED",
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@eslint/js": "9.30.1",
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"find-cache-directory": "^6.0.0",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"dependencies": {
		"@preact/signals-core": "^1.12.2",
		"obsidian": "latest"
	}
